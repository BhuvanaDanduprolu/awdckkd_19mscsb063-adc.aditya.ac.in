import java.util.*;
class Fibonacci
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
System.out.print("enter the number: ");
int n=sc.nextInt();
int temp,a=0,b=1;
System.out.print(a+" "+b+" ");
for(int i=2;i<=n;i++)
{
temp=a+b;
a=b;
b=temp;
System.out.print(b+" ");
}
}
}
----------------------------------------------
output:
enter the number: 5
0 1 1 2 3 5
