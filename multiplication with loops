public class Solution {
    public static void main(String[] args) throws IOException {
        int n=2,i=1;
        while(i<=10)
        {
            System.out.println(n + " * " + i + " = " + n*i);
            i++;
        }
        -----------------
output:
       2 * 1 = 2 
       2 * 2 = 4 
       2 * 3 = 6
       2 * 4 = 8 
       2 * 5 = 10 
       2 * 6 = 12
       2 * 7 = 14
       2 * 8 = 16
       2 * 9 = 18
       2 * 10 = 20  
